{
  "name": "save-anywhere",
  "description": "chrome 浏览器插件 - 任何位置任何页面保存选中的文本",
  "version": "1.0.18",
  "homepage": "https://github.com/hnjd-fe/chrome-plugin-save-anywhere",
  "author": "suches<suches@btbtd.org>",
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "scripts": {
    "build": "rm -rf ./save-anywhere && webpack --config scripts/config/webpack.config.prod.js && cp ./src/manifest.json ./save-anywhere && npm run finishbuild",
    "serve": "cross-env NODE_ENV=development webpack-dev-server --config scripts/config/webpack.config.dev.js --hot --inline --host 0.0.0.0 --disable-host-check --history-api-fallback",
    "finishbuild": "sh ./finishbuild.sh",
    "backup": "cp -rf ./save-anywhere/chrome ./src/ && cp -rf ./save-anywhere/manifest.json ./src/manifest.json",
    "packzip": "rm -rf save-anywhere.zip && zip -r save-anywhere.zip ./save-anywhere"
  },
  "dependencies": {
    "awesome-typescript-loader": "^5.2.1",
    "axios": "^0.19.0",
    "compile-ejs-loader": "^0.0.2",
    "deepmerge": "^4.0.0",
    "ejs-loader": "^0.3.1",
    "ejs-render-loader": "^1.0.0",
    "element-ui": "^2.10.0",
    "happypack": "^5.0.0",
    "html-loader": "^0.5.5",
    "less": "3.8.1",
    "less-loader": "4.1.0",
    "lodash.merge": "^4.6.1",
    "parseboolean": "1.0.0",
    "progress-bar-webpack-plugin": "^1.11.0",
    "qs": "6.5.2",
    "request": "^2.88.0",
    "reset-css": "^4.0.1",
    "vue": "^2.6.10",
    "vue-axios": "^2.1.4",
    "vue-i18n": "^8.11.2",
    "vue-loader": "^15.7.0",
    "vue-router": "^3.0.6",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@types/node": "~9.4.0",
    "@types/webpack-env": "~1.13.5",
    "autoprefixer": "~7.2.5",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.3",
    "babel-jest": "22.4.3",
    "babel-loader": "7",
    "babel-plugin-dva-hmr": "0.4.1",
    "babel-plugin-import": "1.5.0",
    "babel-plugin-preval": "1.5.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-syntax-jsx": "6.18.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "^1.0.0",
    "babel-preset-stage-0": "6.24.1",
    "babel-runtime": "6.26.0",
    "compression-webpack-plugin": "~1.1.6",
    "copy-webpack-plugin": "~4.3.1",
    "cross-env": "5.2.0",
    "css-loader": "^1.0.1",
    "cssnano": "~3.10.0",
    "es6-promise": "~4.2.4",
    "extract-text-webpack-plugin": "~3.0.2",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "~2.30.1",
    "json-loader": "~0.5.7",
    "minimist": "~1.2.0",
    "optimize-css-assets-webpack-plugin": "~3.2.0",
    "postcss-loader": "^3.0.0",
    "raw-loader": "~0.5.1",
    "style-loader": "~0.20.1",
    "url-loader": "~0.6.2",
    "vue-hot-reload-api": "~2.2.4",
    "webpack": "~3.10.0",
    "webpack-dev-server": "~2.11.1"
  }
}
